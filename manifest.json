{
  "name": "New Productivity Tab",
  "description": "Overrides the new tab page with the New Productivity Tab",
  "version": "0.1",
  "chrome_url_overrides": {
    "newtab": "newProductivityTab.html"
  },
  "manifest_version": 2,
  "permissions":["tabs","background", "tts","alarms", "https://code.jquery.com/jquery-3.5.1.slim.min.js" ],
  "background": { "scripts": ["scripts/common.js", "scripts/background.js"] },
  "content_security_policy": "script-src 'self' https://code.jquery.com/jquery-3.5.1.slim.min.js; object-src 'self'"
  
}
